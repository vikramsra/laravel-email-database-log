{
    "name": "vikramsra/laravel-email-database-log",
    "description": "A simple database logger for all outgoing emails sent by Laravel website.",
    "keywords": ["laravel", "markdown", "mail"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "doctrine/dbal": "^3.3",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-0": {
            "vikramsra\\LaravelEmailDatabaseLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "vikramsra\\LaravelEmailDatabaseLog\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "vikramsra\\LaravelEmailDatabaseLog\\LaravelEmailDatabaseLogServiceProvider"
            ]
        }
    }
}
